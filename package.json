{
  "name": "react-native-terminal-component",
  "version": "1.0.5",
  "description": "Emulate a terminal environment in React Native",
  "scripts": {
    "build-rn": "node ./react-native.wrapper",
    "build": "webpack --mode development && webpack --mode production",
    "test": "cross-env NODE_PATH=./src mocha --require babel-core/register --colors './test/**/*.spec.js' './demo-**/**/*.spec.js'",
    "test:min": "yarn run test --reporter min",
    "test:coverage": "nyc yarn run test",
    "cli": "node demo-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cawfree/react-native-javascript-terminal.git"
  },
  "keywords": [
    "react",
    "react-native",
    "javascipt",
    "shell",
    "command",
    "line",
    "terminal",
    "emulation"
  ],
  "author": "Alex Thomas (@cawfree)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cawfree/react-native-terminal-component/issues"
  },
  "homepage": "https://github.com/Cawfree/react-native-terminal-component",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.3.10",
    "chai-immutable": "^2.0.0-alpha.1",
    "chai-spies": "^1.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.33.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "webpack": "5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/preset-env": "^7.23.5",
    "get-options": "^1.1.1",
    "immutable": "^5.0.0-beta.4",
    "js-string-escape": "^1.0.1",
    "minimatch": "^9.0.3",
    "minimatch-capture": "^1.1.0",
    "node-gyp": "9.4.0"
  }
}
